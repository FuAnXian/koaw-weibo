<div class="d-flex">
    <div class=""></div>
    <ul class="pagination ml-auto">
        <li class="page-item <%= page.first ? 'disabled' :'' %>"><a class="page-link" href="/?pageIndex=<%= page.active-1 %>">上一页</a></li>
        <%page.num.map((item)=>{%>
            <li class="page-item <%= item == page.active ? 'active' : ''%>"><a class="page-link" href="/?pageIndex=<%=item %>"><%= item %></a></li>
        <%})%>
        <li class="page-item <%= page.last ? 'disabled' :'' %>"><a class="page-link" href="/?pageIndex=<%= page.active+1 %>">下一页</a></li>
    </ul>
</div>

